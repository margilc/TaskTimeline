        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "dependency-cruiser": "^16.10.0",
        "esbuild": "^0.25.5",
        "jest": "^29.7.0",
        "obsidian": "latest",
        "ts-jest": "^29.2.5",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    },
